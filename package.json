{
  "name": "@dsanchez.co/reisift-sdk",
  "version": "0.3.0",
  "description": "TypeScript SDK for the REISift API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:check": "tsc --noEmit",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.scripts.json --noEmit",
    "typecheck:src": "tsc --noEmit",
    "typecheck:scripts": "tsc -p tsconfig.scripts.json --noEmit",
    "prepublishOnly": "npm run build",
    "parse-har": "tsx scripts/parse-har.ts",
    "smoke-test": "tsx scripts/smoke-test.ts"
  },
  "keywords": [
    "reisift",
    "api",
    "sdk",
    "typescript"
  ],
  "author": "Daniel Sanchez",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danicanod/reisift-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/danicanod/reisift-sdk/issues"
  },
  "homepage": "https://github.com/danicanod/reisift-sdk#readme",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "dotenv": "^17.2.3",
    "tsx": "^4.7.0",
    "typescript": "^5.5.0"
  }
}
